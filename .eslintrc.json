{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        // "linebreak-style": "off",
        "no-param-reassign": "off", 
        // "operator-linebreak": "off", 
        // "implicit-arrow-linebreak": "off", 
        // "object-curly-newline": "off", 
        // "comma-dangle": "off",
        "no-unused-expressions": "off",
        "import/extensions": "off"
    }
}
